\n    favs: this.props.fav\r\n  };\r\n  componentDidMount() {\r\n  \r\n    const favs = this.state.favs;\r\n    favs.forEach(element => {\r\n      \r\n      if(element.post._id === this.state.post._id){\r\n       \r\n        const post = this.state.post;\r\n        post.liked = true;\r\n        this.setState({post})\r\n\r\n      }\r\n    });\r\n  \r\n\r\n \r\n  }\r\n  handleLike = () => {\r\n    const post = this.state.post;\r\n    if(!post.liked){\r\n      makeFavourite(post._id);\r\n    post.total_likes +=1;\r\n    post.liked = !post.liked;\r\n    }  \r\n    \r\n    this.setState({ post });\r\n  };\r\n  handleDelete = () => {\r\n    const post = this.state.post;\r\n    this.props.onPostDelete(post);\r\n  };\r\n  handleUpdate = () => {\r\n    const post = this.state.post;\r\n    this.props.onPostUpdate(post);\r\n  };\r\n  render() {\r\n    return (\r\n      <div className=\"container\">\r\n        <div className=\"row\">\r\n          <div className=\"card mb-3\" style={{ width: \"100%\" }}>\r\n            <div className=\"row no-gutters\">\r\n              \r\n              <div className=\"col-md-12\">\r\n                <div className=\"card-body\">\r\n                  <h5 className=\"card-title\">{this.props.value.item_name}</h5>\r\n\r\n                  {this.props.value.author && (\r\n                    <h6 className=\"text-muted\">\r\n                      {this.props.value.author.name}\r\n                    </h6>\r\n                  )}\r\n                  <p>\r\n                    <small className=\"text-muted\">\r\n                      {Moment(this.props.value.createdAt).format(\r\n                        \"D-MMM-YYYY hh:mm:ss\"\r\n                      )}\r\n                    </small>\r\n                  </p>\r\n                  <h6>\r\n                    <span className=\"badge badge-secondary\">\r\n                      {this.props.value.cuisine}\r\n                    </span>\r\n                  </h6>\r\n                  <p className=\"card-text\">{this.props.value.review}</p>\r\n                  <span>\r\n                    <Like\r\n                      liked={this.state.post.liked}\r\n                      onClick={this.handleLike}\r\n                    />\r\n                   {this.props.value.total_likes}\r\n                    {/*<Comment />\r\n                    10*/}\r\n                  </span>\r\n                </div>\r\n                {(this.state.user.isAdmin ||\r\n                  (this.props.value.author &&\r\n                    this.state.user._id === this.props.value.author._id)) && (\r\n                  <div className=\"card-footer text-muted\">\r\n                    <Link to={`/UpdatePost/${this.props.value._id}`}>\r\n                      <span className=\"btn btn-outline-primary btn-sm\">Update</span>\r\n                    </Link>\r\n                    <a\r\n                      onClick={this.handleDelete}\r\n                      className=\"btn btn-outline-danger btn-sm\"\r\n                      style={{ marginLeft: \"5px\" }}\r\n                    >\r\n                      Delete\r\n                    </a>\r\n                  </div>\r\n                )}\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\nexport default Post;\r\n","C:\\Users\\91758\\Desktop\\react\\New folder\\divine\\src\\index.js",[],"C:\\Users\\91758\\Desktop\\react\\New folder\\divine\\src\\components\\common\\like.jsx",[],"C:\\Users\\91758\\Desktop\\react\\New folder\\divine\\src\\components\\common\\comment.jsx",[],"C:\\Users\\91758\\Desktop\\react\\New folder\\divine\\src\\components\\common\\pagination.jsx",["113"],"import React from 'react';\r\nimport _ from 'lodash';\r\n\r\nconst Pagination = (props) => {\r\n    \r\n    const {itemsCount, pageSize,onPageChange} = props;\r\n    const pagesCount = Math.ceil(itemsCount/pageSize);\r\n    \r\n    const pages = _.range(1, pagesCount+1);\r\n    return <nav>\r\n       